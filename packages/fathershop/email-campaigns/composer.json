{
    "name": "fathershop/email-campaigns",
    "description": "Laravel package for managing and sending email campaigns to customers with filtering capabilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rana Subhan",
            "email": "ranasubhan8601@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FatherShop\\EmailCampaigns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FatherShop\\EmailCampaigns\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FatherShop\\EmailCampaigns\\Providers\\EmailCampaignsServiceProvider"
            ],
            "aliases": {
                "EmailCampaigns": "FatherShop\\EmailCampaigns\\Facades\\EmailCampaigns"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}